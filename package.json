{
  "name": "inventarius",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "pm2 start dist/app.js",
    "stop": "pm2 stop dist/app.js",
    "update": "pm2 update dist/app.js",
    "compile": "tsc && node app.js",
    "start:dev": "concurrently --kill-others \"tsc -w\" \"nodemon dist/app.js\"",
    "start:prod": "rm -rf dist/* && tsc && pm2 start dist/app.js",
    "docker:dev": "docker compose -f docker-compose.dev.yml up",
    "docker:prod": "docker compose -f docker-compose.prod.yml up",

    "migrate": "cd migrations && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.uplab.digital/uplabteam/inventarius/inventarius.backend.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "20": "^3.1.9",
    "@decorators/express": "^3.0.0",
    "@sequelize/core": "^7.0.0-alpha.29",
    "concurrently": "^8.2.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "express-winston": "^4.2.0",
    "mongodb": "6.0",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "pg-promise": "^11.5.4",
    "pm2": "^5.3.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.34.0",
    "typescript": "^5.2.2",
    "winston": "^3.10.0",
    "winston-mongodb": "^5.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.6.0",
    "sequelize-cli": "^6.6.2"
  }
}
